@model IEnumerable<EMSApp.Models.USER_INFO>

@{
    ViewBag.Title = "All User";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <p class="btn btn-primary pull-left">
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.USER_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CONTACT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.USER_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PASSWORD)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.USER_LEVEL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IS_DELETED)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ACTION_DATE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ACTION_BY)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UPDATE_DATE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UPDATE_BY)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EMPLOYEE_INFO.EMPLOYEE_NAME)
            </th>
            <th></th>
        </tr>

        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.USER_NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CONTACT)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.USER_ID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PASSWORD)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.USER_LEVEL)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IS_DELETED)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ACTION_DATE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ACTION_BY)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UPDATE_DATE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UPDATE_BY)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EMPLOYEE_INFO.EMPLOYEE_NAME)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                        @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.ID })
                    </td>
                </tr>
            }

        }

    </table>

</div>
