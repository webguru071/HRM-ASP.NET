@model IEnumerable<EMSApp.Models.NOTICE_BOARD>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Action
        </th>
        <th>
            Notice
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink("View", "Edit", new { id = item.ID })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOTICE)
            </td>
        </tr>
    }

</table>
