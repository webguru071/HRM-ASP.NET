@model IEnumerable<EMSApp.Models.ASSET_MANAGEMENT>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>Action</th>
        <th>
            Employee
        </th>

        <th>
           Equipment
        </th>
        <th>
            Assigned
        </th>
        <th>
            Returned
        </th>
        <th>
            Remarks
        </th>

    </tr>

    @if (Model != null)
    {
        foreach (var item in Model)
        {
<tr>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.ASSET_MNG_ID }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.ASSET_MNG_ID })
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EMPLOYEE_INFO.EMPLOYEE_NAME)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.EQUEPMENTS_INFO.EQP_TITLE)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DATE_ASSN)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DATE_RTN)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.REMARKS)
    </td>

</tr>
        }
    }

</table>
